<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="model.Zadanie" table="ZADANIE">
        <id name="id" type="int">
            <column name="ID"/>
            <generator class="native"/>
        </id>
        <property name="nazwa" type="java.lang.String"/>
        <property name="opis" type="java.lang.String"/>
        <many-to-one cascade="save-update" class="model.Misja" column="misja_id" foreign-key="FK_ZADANIE_MISJA"
                     name="misja" unique="false"/>
    </class>
</hibernate-mapping>
