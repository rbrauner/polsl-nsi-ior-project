<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="model.Ladunek" table="LADUNEK">
        <id name="id" type="int">
            <column name="ladunek_id"/>
            <generator class="native"/>
        </id>
        <property name="typ">
            <type name="org.hibernate.type.EnumType">
                <param name="enumClass">model.LadunekTyp</param>
            </type>
        </property>
        <property name="uwagi" type="java.lang.String"/>
        <many-to-one cascade="save-update" class="model.LotTransportowy" column="misja_id" foreign-key="FK_LADUNEK_MISJA"
                     name="misja" unique="false"/>
    </class>
</hibernate-mapping>
