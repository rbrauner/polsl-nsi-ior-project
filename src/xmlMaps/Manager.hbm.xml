<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <joined-subclass extends="model.Employee" lazy="false" name="model.Manager" 
  table="MANAGERS">
    <key foreign-key="FK_MAN_EMP">
      <column name="MAN_ID"/>
    </key>
    <property name="salSup" type="double"/>
   </joined-subclass>
</hibernate-mapping>
